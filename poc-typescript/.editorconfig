# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.{js, ts}]
indent_style = space
indent_size = 4

[{package.json,.travis.yml, tslint.json, tsconfig.json}]
indent_style = space
indent_size = 2